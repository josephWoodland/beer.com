{
  "name": "beer.com",
  "version": "1.0.0",
  "description": "landing page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch assets/sass/main.scss:assets/css/style.css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass assets/sass/main.scss assets/css/style.comp.css",
    "concat:css": "concat -o asstes/css/style.concat.css assets/css/icon-font.css assets/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' assets/css/style.concat.css -o assets/css/style.prefix.css",
    "compress:css": "node-sass assets/css/style.prefix.css assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Joseph",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "sass": "^1.32.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josephWoodland/beer.com.git"
  },
  "bugs": {
    "url": "https://github.com/josephWoodland/beer.com/issues"
  },
  "homepage": "https://github.com/josephWoodland/beer.com#readme"
}
